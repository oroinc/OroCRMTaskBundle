<script type="text/html" class="reminder_templates" data-identifier="task_template">
<% if (url != '') { %>
    <span><a class="hash-navigation-link" href="<%- url %>"><%- subject %></a> is due on <%- expireAt %></span>
<% } else { %>
    <span><%- subject %> is due on <%- expireAt %></span>
<% } %>
</script>
